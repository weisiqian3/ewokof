{
  "name": "ewokof",
  "version": "0.0.1",
  "description": "CloudFlare R2 storage manager with Pages and Workers.",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "deploy": "wrangler deploy",
    "format": "prettier --write \"./(frontend|backend)/**/*.{js,jsx,ts,tsx,json,css,scss,vue,md}\"",
    "user:create": "bun scripts/create-user.ts",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:migrate": "drizzle-kit migrate",
    "drizzle:push": "wrangler d1 migrations apply D1 --local",
    "drizzle:push-prod": "cross-env DRIZZLE_STUDIO_MODE=prod drizzle-kit migrate",
    "drizzle:studio": "cross-env DRIZZLE_STUDIO_MODE=local drizzle-kit studio",
    "drizzle:studio-prod": "cross-env DRIZZLE_STUDIO_MODE=prod drizzle-kit studio"
  },
  "repository": {
    "type": "git",
